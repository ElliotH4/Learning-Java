public class JavaClass {
	public static void main(String[] args) {

		System.out.println(Math.abs(-10.5)); //Absolute Value turns values positive
		System.out.println(Math.ceil(7.3)); //Ceiling rounds up
		System.out.println(Math.floor(7.3)); //Floor rounds down
		System.out.println(Math.max(8.6, 5.2)); //Max chooses the bigger number
		System.out.println(Math.min(8.6, 5.2)); //Min chooses the smaller number
		System.out.println(Math.pow(5,3)); //Power makes it 5 to the 3rd
		System.out.println(Math.sqrt(9)); //Square root is the square root of the number
		
	}
}
