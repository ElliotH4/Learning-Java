import java.util.Scanner;

public class Practice {
	public static void main(String[] args) {
		
		Scanner scr = new Scanner(System.in);
		int total = 0;
		int counter = 0;
		int grade;
		int average;
		
		while (counter < 10) {
			grade = scr.nextInt();
			total = total + grade;
			counter++;
		}
		
		average = total / 10;
		System.out.println("Your average is " + average);
		
	}
}
